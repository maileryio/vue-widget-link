{
  "name": "@maileryio/vue-widget-link",
  "version": "1.0.4",
  "description": "Vue Widget Link",
  "license": "BSD-3-Clause",
  "main": "dist/main.umd.js",
  "module": "dist/main.esm.js",
  "unpkg": "dist/main.min.js",
  "browser": {
    "./sfc": "src/main.vue"
  },
  "scripts": {
    "lint": "eslint .",
    "fix": "eslint --fix .",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "cov": "echo \"Error: no codecov specified\" && exit 1",
    "ci": "npm run lint && npm run cov",
    "test:unit": "vue-cli-service test:unit test/**/*.test.js",
    "build": "yarn run build:umd & yarn run build:es & yarn run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/main.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/main.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/main.min.js"
  },
  "dependencies": {},
  "devDependencies": {
    "@vue/cli-plugin-unit-mocha": "^4.2.2",
    "@vue/cli-service": "^4.2.2",
    "@vue/test-utils": "^1.0.0-beta.31",
    "chai": "^4.2.0",
    "conventional-changelog-cli": "^2.0.31",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-chai-friendly": "^0.5.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.1.2",
    "rollup": "^1.31.0",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-vue": "^5.1.6",
    "vue-template-compiler": "^2.6.11"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "ci": {
    "version": "8, 9"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maileryio/vue-widget-link.git"
  },
  "bugs": {
    "url": "https://github.com/maileryio/vue-widget-link/issues"
  },
  "homepage": "https://github.com/maileryio/vue-widget-link#readme"
}
